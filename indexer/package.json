    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/schema": "^10.0.6",
    "@kadena/chainweb-stream-client": "^0.2.2",
    "@kadena/client": "^1.11.1",
    "@kadena/types": "^0.6.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "apollo-server": "^3.13.0",
    "axios": "^1.6.5",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postgraphile": "^4.13.0",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "prom-client": "^15.1.0",
    "sequelize": "^6.37.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "ws": "^8.16.0"
  },
  "name": "@kadena-indexer/indexer",
  "description": "Indexer for the Kadena blockchain",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "dotenv": "^16.4.4",
    "nodemon": "^3.1.4"
  },
  "scripts": {
    "build": "tsc",
    "generate": "graphql-codegen",
    "dev:streaming": "ts-node src/index.ts --streaming",
    "dev:backfill": "ts-node src/index.ts --backfill",
    "dev:missing": "ts-node src/index.ts --missing",
    "dev:headers": "ts-node src/index.ts --headers",
    "dev:payloads": "ts-node src/index.ts --payloads",
    "dev:graphql": "ts-node src/index.ts --graphql",
    "dev:graphqlserver": "nodemon src/index.ts --graphqlServer",
    "dev:run": "ts-node src/index.ts --run",
    "prod:start": "docker-compose up --build indexer && docker-compose logs -f indexer",
    "prod:streaming": "node dist/index.js --streaming",
    "prod:backfill": "node dist/index.js --backfill"
  },
